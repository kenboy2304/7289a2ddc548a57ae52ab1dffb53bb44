
@using DNS.WebAPI.Models

@{
    ViewBag.Title = "Quản lý Kinh Thánh Hàng Ngày";
}



<table class="table table-striped table-condensed table-bordered" style="text-align: center">
    <thead>
        <tr>
            <th>Tháng</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach(Month m in Enum.GetValues(typeof(Month)))
        {
            <tr>
                <td>@Html.DisplayFor(item=> m )</td>
                <td><a class="btn btn-circle btn-primary" href="@Url.Action("Edit",new {month=(int)m})"><i class="fa fa-edit"></i></a></td>
            </tr>
            
        }
    </tbody>
</table>


